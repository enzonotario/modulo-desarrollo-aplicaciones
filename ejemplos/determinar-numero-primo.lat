escribir("Ingresa un número")

numero = leer()
j = 0

desde (i = 1; i <= numero; i++)
    si (numero % i  == 0)
        j = j + 1
    fin

    si (j > 2)
        romper
    fin
fin

si (j == 2 || numero  == 1)
    escribir("El número " .. numero .. " es primo")
sino
    escribir("El número " .. numero .. " no es primo")
fin